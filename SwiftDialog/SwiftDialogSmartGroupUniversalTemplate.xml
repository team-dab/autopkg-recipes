<computer_group>
	<name>%GROUP_NAME%</name>
	<is_smart>true</is_smart>
	<criteria>
		<criterion>
			<name>SwiftDialog Installed</name>
			<priority>0</priority>
			<and_or>and</and_or>
			<search_type>is</search_type>
			<value>Yes</value>
		</criterion>
		<criterion>
			<name>SwiftDialog Version</name>
			<priority>1</priority>
			<and_or>and</and_or>
			<search_type>does not match regex</search_type>
			<value>%version_regex%</value>
		</criterion>
	</criteria>
</computer_group>
